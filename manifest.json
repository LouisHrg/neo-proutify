{
  "manifest_version": 2,
  "name": "Neo-Proutify",
  "description": "Remplace des nom et des mots par des prouts.",
  "version": "1.0",
  "homepage_url": "https://github.com/louishrg/neo-proutify/",
  "icons": {
      "64": "static/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "js/replace.js"
      ]
    }
  ],"browser_action": {
      "default_icon": {
      "64": "static/icon.png"
    },
      "default_popup": "index.html"
  }
}
