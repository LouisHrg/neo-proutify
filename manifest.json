{
  "manifest_version": 2,
  "name": "Proutify",
  "description": "Remplace le noms de personnalités politiques française par leurs versions proutifiées.",
  "version": "1.3",
  "homepage_url": "https://framagit.org/framasoft/fun/proutify",
  "icons": {
      "64": "static/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "js/replace.js"
      ]
    }
  ],"browser_action": {
      "default_icon": {
      "64": "static/icon.png"
    },
      "default_popup": "index.html"
  }
}
